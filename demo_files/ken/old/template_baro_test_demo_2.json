                "label": "k$\\mathregular{_{Ca release}}$\n(M$\\mathregular{^{-1}}$ s$\\mathregular{^{-1}}$)",
                "ticks": [0, 0.06],
                "series":
                [
                    {
                        "field": "k_act"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{SERCA}}$\n(M$\\mathregular{^{-1}}$ s$\\mathregular{^{-1}}$)",
                "ticks": [0, 40],
                "series":
                [
                    {
                        "field": "k_serca"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{1}}$\n(s$\\mathregular{^{-1}}$)",
                "ticks": [0, 7],
                "series":
                [
                    {
                        "field": "k_1"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{3}}$\n(nm$\\mathregular{^{-1} s^{-1}}$)",
                "ticks": [0, 400],
                "series":
                [
                    {
                        "field": "k_3"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "k$\\mathregular{_{on}}$\n($\\mathregular{\\mu}$M$\\mathregular{^{-1} s^{-1}}$)",
                "ticks": [0, 700],
                "series":
                [
                    {
                        "field": "k_on",
                        "scaling_factor": 1e-6
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Venous\ncompliance\n(liters (mm Hg)$\\mathregular{^{-1}}$)",
                "ticks": [0, 1.4],
                "series":
                [
                    {
                        "field": "veins_compliance"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Arteries\nresistance\n(mm Hg (liters)$\\mathregular{^{-1}}$ s)",
