{
    "layout":
    {
        "fig_width": 7,
        "left_margin": 0,
        "right_margin": 0,
        "grid_wspace": -1
    },
    "x_display":{
        "global_x_field": "time",
        "label": "Time (s)"
    },
    "formatting":
    {
        "x_label_fontsize": 9,
        "y_label_fontsize": 9,
        "tick_fontsize": 9,
        "legend_fontsize": 6,
        "y_label_pad": 10 
    },
    "panels":
    [
        {
            "column": 1,
            "y_info":
            {
                "label": "Activation",
                "series":
                [
                    {
                        "field": "membrane_activation",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Myofilament\n[$\\mathregular{Ca^{2+}}$]\n($\\mathregular{\\mu}$M)",
                "series":
                [
                    {
                        "field": "Ca_cytosol",
                        "scaling_factor": 1e6,
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Stress\n(kN m$\\mathregular{^{-2}}$)",
                "series":
                [
                    {
                        "field": "hs_force",
                        "field_label": "Total",
                        "scaling_factor": 1e-3,
                        "style": "envelope"
                    },
                    {
                        "field": "pas_force",
                        "field_label": "Passive",
                        "scaling_factor": 1e-3,
                        "style": "envelope"
                    },
                    {
                        "field": "cb_force",
                        "field_label": "Active",
                        "scaling_factor": 1e-3,
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Half-\nsarcomere\nlength\n(nm)",
                "scaling_type": "close_fit",
                "series":
                [
                    {
                        "field": "hs_length",
                        "style": "envelope"
                    },
                    {
                        "field": "hs_length",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Thin\nfilament",
                "series":
                [
                    {
                        "field": "n_on",
                        "field_label": "N$\\mathregular{_{on}}$",
                        "style": "envelope"
                    },
                    {
                        "field": "n_off",
                        "field_label": "N$\\mathregular{_{off}}$",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 1,
            "y_info":
            {
                "label": "Thick\nfilament",
                "series":
                [
                    {
                        "field": "M_SRX",
                        "field_label": "M$\\mathregular{_{SRX}}$",
                        "style": "envelope"
                    },
                    {
                        "field": "M_DRX",
                        "field_label": "M$\\mathregular{_{DRX}}$",
                        "style": "envelope"
                    },
                    {
                        "field": "M_FG",
                        "field_label": "M$\\mathregular{_{FG}}$",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Pressure\n(mm Hg)",
                "series":
                [
                    {
                        "field": "pressure_ventricle",
                        "field_label": "Ventricle",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Volume\n(liters)",
                "series":
                [
                    {
                        "field": "volume_ventricle",
                        "field_label": "Ventricle",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Pressure\n(mm Hg)",
                "series":
                [
                    {
                        "field": "pressure_ventricle",
                        "field_label": "Ventricle",
                        "style": "envelope"
                    },
                    {
                        "field": "pressure_aorta",
                        "field_label": "Aorta",
                        "style": "envelope"
                    },
                    {
                        "field": "pressure_arteries",
                        "field_label": "Arteries",
                        "style": "envelope"
                    },
                    {
                        "field": "pressure_arterioles",
                        "field_label": "Arterioles",
                        "style": "envelope"
                    },
                    {
                        "field": "pressure_capillaries",
                        "field_label": "Capillaries",
                        "style": "envelope"
                    },
                    {
                        "field": "pressure_venules",
                        "field_label": "Venules",
                        "style": "envelope"
                    },
                    {
                        "field": "pressure_veins",
                        "field_label": "Veins",
                        "style": "envelope"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Volume",
                "ticks": [-3,1],
                "series":
                [
                    {
                        "field": "volume_aorta",
                        "field_label": "Aorta",
                        "style": "envelope",
                        "log_display": "on"
                    },
                    {
                        "field": "volume_arteries",
                        "field_label": "Arteries",
                        "style": "envelope",
                        "log_display": "on"
                    }
                ]
            }
        },
        {
            "column": 2,
            "y_info":
            {
                "label": "Flow",
                "series":
                [
                    {
                        "field": "flow_veins_to_ventricle",
                        "field_label": "Veins to ventricle",
                        "style": "envelope"
                    },
                    {
                        "field": "flow_ventricle_to_aorta",
                        "field_label": "Ventricle to aorta",
                        "style": "envelope"
                    },
                    {
                        "field": "flow_aorta_to_arteries",
                        "field_label": "Aorta to arteries",
                        "style": "envelope"
                    },
                    {
                        "field": "flow_arteries_to_arterioles",
                        "field_label": "Arteries to arterioles",
                        "style": "envelope"
                    }
                ]
            }
        }
    ]
}