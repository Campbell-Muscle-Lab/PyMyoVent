{
    "layout":
    {
        "fig_width": 7,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 2,
        "grid_wspace": -0.5
    },
    "x_display":{
        "global_x_field": "time",
        "label": "Time (s)"
    },
    "formatting":
    {
        "x_label_fontsize": 9,
        "y_label_fontsize": 9,
        "tick_fontsize": 9,
        "legend_fontsize": 7,
        "y_label_pad": 25,
        "legend_bbox_to_anchor": [1.0, 1],
        "patch_alpha": 0.3
    },
    "panels":
    [
        {
            "column": 1,
            "y_info":
            {
                "label": "Pressure\n(mm Hg)",
                "series":
                [
                    {
                        "field": "pressure_aorta",
                        "field_label": "Aorta"
                    },
                    {
                        "field": "pressure_arteries",
                        "field_label": "Arterties"
                    },
                    {
                        "field": "pressure_arterioles",
                        "field_label": "Arterioles"
                    },
                    {
                        "field": "pressure_capillaries",
                        "field_label": "Capillaries"
                    },
                    {
                        "field": "pressure_venules",
                        "field_label": "Venules"
                    },
                    {
                        "field": "pressure_veins",
                        "field_label": "Veins"
                    },
                    {
                        "field": "pressure_ventricle",
                        "field_label": "Ventricle"
                    }
                ]
            }
        }
    ]
}