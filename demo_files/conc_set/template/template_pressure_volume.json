{
    "layout":
    {
        "fig_width": 5,
        "top_margin": 0,
        "left_margin": 0,
        "right_margin": 0,
        "panel_height": 5,
        "grid_wspace": -0.5
    },
    "x_display":{
        "global_x_field": "volume_ventricle",
        "label": "Volume (liters)",
        "ticks" :[0,0.2],
        "ticks_rel_to_end":[0,0]
    },
    "formatting":
    {
        "x_label_fontsize": 9,
        "y_label_fontsize": 9,
        "tick_fontsize": 9,
        "legend_fontsize": 7,
        "y_label_pad": 25,
        "legend_bbox_to_anchor": [1.0, 1],
        "patch_alpha": 0.3
    },
    "panels":
    [
        {
            "column": 1,
            "y_info":
            {
                "label": "Pressure\n(mm Hg)",
                "series":
                [
                    {
                        "field": "pressure_ventricle",
                        "field_label": "Ventricle"
                    }
                ]
            }
        }
    ]
}
