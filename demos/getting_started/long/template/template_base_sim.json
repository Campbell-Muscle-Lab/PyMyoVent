                "series":
                [
                    {
                        "field": "M_SRX",
                        "field_label": "SRX",
                        "style": "envelope"
                    },
                    {
                        "field": "M_DRX",
                        "field_label": "DRX",
                        "style": "envelope"
                    },
                    {
                        "field": "M_FG",
                        "field_label": "FG",
                        "style": "envelope"
                    }
                ]
            }
        }
