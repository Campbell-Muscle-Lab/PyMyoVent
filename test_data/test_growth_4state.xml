<?xml version="1.0" encoding="windows-1252"?>
<single_circulation_simulation>

<simulation_parameters>
	<no_of_time_points>400000</no_of_time_points>
	<time_step>0.001</time_step>
	<activation_frequency>1</activation_frequency>
	<duty_ratio>0.05</duty_ratio>
</simulation_parameters>

<output_parameters>
	<data_file>../output/data/summary_data.xlsx</data_file>
	<summary_figure>../output/figures/summary_figure.png</summary_figure>
	<pv_figure>../output/figures/pressure_volume_figure.png</pv_figure>
	<flows_figure>../output/figures/flows_figure.png</flows_figure>
	<hs_fluxes_figure>../output/figures/hs_fluxes_figure.png</hs_fluxes_figure>
</output_parameters>

<perturbations>
	<venous_compliance>
		<start_index>1500000</start_index>
		<stop_index>1510000</stop_index>
		<increment>-5e-5</increment>
	</venous_compliance>
	<aorta_resistance>
		<start_index>500000</start_index>
		<stop_index>501000</stop_index>
		<increment>2e-2</increment>
	</aorta_resistance>
	<k_1>
		<start_index>200000</start_index>
		<stop_index>250000</stop_index>
		<increment>0.00008</increment>
	</k_1>
</perturbations>

<baroreceptor>
	<start_index>10000</start_index>
	<target_aorta_pressure>110</target_aorta_pressure>
	<sensitivity>0.05</sensitivity>
	<hr_gain>0.2</hr_gain>
	<k1_gain>0.000</k1_gain>
	<k3_gain>0.0001</k3_gain>
</baroreceptor>

<growth>
	<memory>7500</memory>
	<eccentric_start_index>30000</eccentric_start_index>
	<eccentric_target>2.5e3</eccentric_target>
	<eccentric_k>-2e-8</eccentric_k>
	<concentric_start_index>100000</concentric_start_index>
	<concentric_target>12</concentric_target>
	<concentric_k>1e-5</concentric_k>
</growth>

<circulation>
	<no_of_compartments>6</no_of_compartments>
	<blood>
		<volume units="liters">5</volume>
	</blood>
	<aorta>
		<resistance units="s">40</resistance>
		<compliance units="liter_per_mmHg">0.002</compliance>
	</aorta>
	<arteries>
		<resistance units="g">200</resistance>
		<compliance units="g2">0.0005</compliance>
	</arteries>
	<arterioles>
		<resistance units="g">500</resistance>
		<compliance units="g2">0.0005</compliance>
	</arterioles>
	<capillaries>
		<resistance units="g">300</resistance>
		<compliance units="g2">0.0025</compliance>
	</capillaries>
	<veins>
		<resistance units="g">100</resistance>
		<compliance units="liters_per_mmHg">0.35</compliance>
	</veins>
	<ventricle>
		<resistance units="g">5</resistance>
		<wall_volume units="liters">0.1</wall_volume>
		<slack_volume units = "liters">0.08</slack_volume>
	</ventricle>
</circulation>

<half_sarcomere>
	<max_rate>5000</max_rate>
	<temperature>288</temperature>
	
	<cb_number_density>6.9e16</cb_number_density>
	<initial_hs_length>900</initial_hs_length>

	<myofilaments>
		<kinetic_scheme>4state_with_SRX</kinetic_scheme>
		<k_1>2</k_1>
		<k_force>1e-3</k_force>
		<k_2>200</k_2>
		<k_3>200</k_3>
		<k_4_0>200</k_4_0>
		<k_4_1>0.1</k_4_1>
		<k_5_0>500</k_5_0>
		<k_5_1>0.1</k_5_1>
		<k_6>0</k_6>
		<k_7_0>100</k_7_0>
		<k_7_1>0.1</k_7_1>
		<k_8>0</k_8>		
		<k_cb>0.001</k_cb>
		<x_ps>5</x_ps>
		<k_on>5e8</k_on>
		<k_off>200</k_off>
		<k_coop>5</k_coop>
		<bin_min>-20</bin_min>
		<bin_max>10</bin_max>
		<bin_width>0.5</bin_width>
		<filament_compliance_factor>0.5</filament_compliance_factor>
		<thick_filament_length>815</thick_filament_length>
		<thin_filament_length>1120</thin_filament_length>
		<bare_zone_length>80</bare_zone_length>
		<k_falloff>0.0024</k_falloff>
		<passive_mode>exponential</passive_mode>
		<passive_exp_sigma>500</passive_exp_sigma>
		<passive_exp_L>80</passive_exp_L>
		<passive_l_slack>900</passive_l_slack>
	</myofilaments>
	
	<membranes>
		<kinetic_scheme>simple_2_compartment</kinetic_scheme>
		<Ca_content>1e-3</Ca_content>
		<k_leak>2e-4</k_leak>
		<k_act>5e-2</k_act>
		<k_serca>8.0</k_serca>
	</membranes>
</half_sarcomere>

</single_circulation_simulation>